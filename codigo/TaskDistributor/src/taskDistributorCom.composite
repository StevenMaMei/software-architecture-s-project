<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
	xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1" targetNamespace="http://frascati.ow2.org/mqcafe"
	name="taskDistributorCom">

	<component name="c">

		<implementation.java class="model.TaskDistributorImp" />
		
		<service name="taskDistributor">
	      <interface.java interface="interfaces.TaskDistributor"/>
	      <frascati:binding.rmi host="localhost" serviceName="rmiServiceDistributor" port="1103"> </frascati:binding.rmi>
	    </service>
	    
	    <service name="subject">
	      <interface.java interface="interfaces.Subject"/>
	      <frascati:binding.rmi host="localhost" serviceName="rmiServiceSubject" port="1104"> </frascati:binding.rmi>
	    </service>
		
	</component>


</composite>
